#! /bin/sh

# Small wrapper to run net-test console, which depends on m0mero module

if [ "$(id -u)" -ne 0 ]; then
    echo "Must be run as root"
    exit 1
fi

. @SRCDIR@/m0t1fs/linux_kernel/st/common.sh

unload_all() {
    modunload
    modunload_galois
}
trap unload_all EXIT

modprobe_lnet
modload_galois
modload || exit $?

@SRCDIR@/net/test/user_space/ntn "$@"
