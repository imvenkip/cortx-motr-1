NET_TEST_SRCDIR	:= @ABS_SRCDIR@/net/test/linux_kernel
NET_TEST	:= m0nettestd

shared_src	:=
kernel_src	:= node_k.c

obj-m		:= $(NET_TEST).o
$(NET_TEST)-y	:= $(shared_src:.c=.o) $(kernel_src:.c=.o)
orig		:= $(shared_src:%=../%)

EXTRA_CFLAGS	= -DHAVE_CONFIG_H -I@top_srcdir@ \
		  @M0_KCPPFLAGS@ @M0_KCFLAGS@ @KCFLAGS@

prepare:
	true

clean distclean:
	$(RM) $(shared_src)
	$(MAKE) -C @LINUX_OBJ@ M=`pwd` clean
