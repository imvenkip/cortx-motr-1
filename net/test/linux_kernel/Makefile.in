NET_TEST_SRCDIR	:= @SRCDIR@/net/test/linux_kernel
NET_TEST	:= net_test_node

shared_src	:=
kernel_src	:= node_k.c

obj-m		:= $(NET_TEST).o
$(NET_TEST)-y	:= $(shared_src:.c=.o) $(kernel_src:.c=.o)

EXTRA_CFLAGS = -DHAVE_CONFIG_H -I@SRCDIR@ @KCFLAGS@

prepare:
	true

clean distclean:
	$(RM) $(shared_src) Module.markers Module.symvers modules.order
	$(MAKE) -C @LINUX_OBJ@ M=`pwd` clean
