#!/usr/bin/awk -f

# Resulting ELF image sizes:
# funcs   text     data    bss       total     filename
# 1281959 3047409  192880  7907136   11147425  libmero.so
# 969554  2805724  171104  7899648   10876476  libmero-altogether.so
# 610121  1044956  73872   56678512  57797340  m0mero.ko

{
    name = $6;
    if (name != "filename") {
        if (total[name] == 0) {
            total[name] = $5;
            bss[name]   = $4;
            data[name]  = $3;
            text[name]  = $2;
            funcs[name] = $1;
        } else {
            total[name] -= $5;
            bss[name]   -= $4;
            data[name]  -= $3;
            text[name]  -= $2;
            funcs[name] -= $1;
        }
    }
}

END {
    print "Bloated-by:";
    print "<size>";
    printf("    funcs    text     data      bss    total filename\n");
    for (name in total) {
        printf("%8d %8d %8d %8d %8d %s\n",
               funcs[name], text[name], data[name], bss[name],
               total[name], name);
    }
    print "</size>";
}
