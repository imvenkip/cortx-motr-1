From 5e5da80f9a31a0f243228ea61a814c0da83ae5f9 Mon Sep 17 00:00:00 2001
From: "Valery V. Vorotyntsev" <valery_vorotyntsev@xyratex.com>
Date: Wed, 23 May 2012 23:50:08 +0300
Subject: [PATCH 189/311] conf/onwire.ff: update export list; align fields

---
 conf/onwire.ff | 22 +++++++++++++---------
 1 file changed, 13 insertions(+), 9 deletions(-)

diff --git a/conf/onwire.ff b/conf/onwire.ff
index 61ed087..9417b52 100644
--- a/conf/onwire.ff
+++ b/conf/onwire.ff
@@ -23,6 +23,10 @@ struct c2_conf_buf;
 struct c2_conf_pathcomp;
 struct c2_conf_fetch;
 struct c2_conf_fetch_resp;
+struct c2_conf_update;
+struct c2_conf_update_resp;
+struct c2_conf_stat;
+struct c2_conf_stat_resp;
 
 /**
  * Sequence of bytes.
@@ -198,12 +202,12 @@ DEF(c2_conf_update, RECORD,
     /** Configuration object the path originates from. */
     _(f_origin, objid),
     /** A sequence of configuration object descriptors. */
-    _(fr_data, enconf));
+    _(fr_data,  enconf));
 
 /** Confd's response to c2_conf_update. */
 DEF(c2_conf_update_resp, RECORD,
     /** Result of update request (-Exxx = failure, 0 = success). */
-    _(fr_rc,   U32));
+    _(fr_rc, U32));
 
 /** Statistics request. */
 DEF(c2_conf_stat, RECORD,
@@ -215,16 +219,16 @@ DEF(c2_conf_stat_resp, RECORD,
     /** Configuration cache entries count. */
     _(f_cache_ent_cnt, U32),
     /** Processed configuration requests count of configuration service. */
-    _(f_req_cnt, U32),
+    _(f_req_cnt,       U32),
     /** Processed update requests count of configuration service. */
-    _(f_upd_cnt, U32),
+    _(f_upd_cnt,       U32),
     /** Processed configuration requests per second. */
-    _(f_req_per_sec, U32),
+    _(f_req_per_sec,   U32),
     /** Processed update requests per second. */
-    _(f_upd_per_sec, U32),
+    _(f_upd_per_sec,   U32),
     /** Maximum time, update request waits to be applied */
-    _(f_waiter_max, U32),
+    _(f_waiter_max,    U32),
     /** An average time, update request waits to be applied */
-    _(f_waiter_avg, U32),
+    _(f_waiter_avg,    U32),
     /** Result of update request (-Exxx = failure, 0 = success). */
-    _(fr_rc,   U32));
+    _(fr_rc,           U32));
-- 
