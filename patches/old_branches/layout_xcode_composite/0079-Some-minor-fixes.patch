diff --git a/layout/layout_internal.h b/layout/layout_internal.h
index b571493..2ac036e 100644
--- a/layout/layout_internal.h
+++ b/layout/layout_internal.h
@@ -101,9 +101,7 @@ enum {
 	L_TABLE_INSERT_ERR         = -507
 };
 
