+	io->si_user.ov_vec.v_nr    = 1;
+	io->si_user.ov_vec.v_count = count;
+	io->si_user.ov_buf         = (void **) seg->bs_addr;
 
-	io->si_stob.iv_vec.v_nr		= 1;
-	io->si_stob.iv_vec.v_count	= count;
-	io->si_stob.iv_index		= BE_SEG_DEFAULT_OFFSET;
+	io->si_stob.iv_vec.v_nr    = 1;
+	io->si_stob.iv_vec.v_count = count;
+	io->si_stob.iv_index       = BE_SEG_DEFAULT_OFFSET;
 }
 
