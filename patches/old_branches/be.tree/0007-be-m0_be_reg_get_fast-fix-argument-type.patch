From d54cc73f209bc94fe9477f4559c2cdee5e190cb5 Mon Sep 17 00:00:00 2001
From: "Valery V. Vorotyntsev" <valery_vorotyntsev@xyratex.com>
Date: Wed, 29 May 2013 12:00:55 +0300
Subject: [PATCH 007/290] be (m0_be_reg_get_fast): fix argument type

---
 be/seg.c | 8 +++-----
 be/seg.h | 2 +-
 2 files changed, 4 insertions(+), 6 deletions(-)

diff --git a/be/seg.c b/be/seg.c
index 9034018..0bc5e9c 100644
--- a/be/seg.c
+++ b/be/seg.c
@@ -44,10 +44,8 @@ void m0_be_seg_fini(struct m0_be_seg *seg)
 
 bool m0_be__reg_invariant(const struct m0_be_reg *reg)
 {
-	return reg != NULL &&
-	       reg->br_seg != NULL &&
-	       reg->br_size > 0 &&
-	       reg->br_addr != NULL;
+	return reg != NULL && reg->br_seg != NULL && reg->br_size > 0 &&
+		reg->br_addr != NULL;
 }
 
 void m0_be_reg_get(struct m0_be_reg *reg, struct m0_be_op *op)
@@ -55,7 +53,7 @@ void m0_be_reg_get(struct m0_be_reg *reg, struct m0_be_op *op)
 	M0_PRE(m0_be__reg_invariant(reg));
 }
 
-void m0_be_reg_get_fast(const struct m0_be_seg *reg)
+void m0_be_reg_get_fast(const struct m0_be_reg *reg)
 {
 	M0_PRE(m0_be__reg_invariant(reg));
 }
diff --git a/be/seg.h b/be/seg.h
index 143162b..fc5d3db 100644
--- a/be/seg.h
+++ b/be/seg.h
@@ -74,7 +74,7 @@ struct m0_be_reg {
  */
 void m0_be_reg_get(struct m0_be_reg *reg, struct m0_be_op *op);
 
-void m0_be_reg_get_fast(const struct m0_be_seg *reg);
+void m0_be_reg_get_fast(const struct m0_be_reg *reg);
 
 void m0_be_reg_put(const struct m0_be_reg *reg);
 
