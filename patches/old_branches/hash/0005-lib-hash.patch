From 1513bdcc1740756e27cb4de24bf08e0ff8fe1174 Mon Sep 17 00:00:00 2001
From: "anand.vidwansa" <anand_vidwansa@xyratex.com>
Date: Tue, 18 Jun 2013 04:00:02 -0700
Subject: [PATCH 05/10] lib:hash - Added hash unit tests in alphabetical order
 in lib ut test suite.

---
 lib/linux_kernel/ut/main.c | 2 +-
 lib/ut/main.c              | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/lib/linux_kernel/ut/main.c b/lib/linux_kernel/ut/main.c
index 9619ba5..1beeb6f 100644
--- a/lib/linux_kernel/ut/main.c
+++ b/lib/linux_kernel/ut/main.c
@@ -64,6 +64,7 @@ const struct m0_test_suite m0_klibm0_ut = {
 #ifdef ENABLE_FAULT_INJECTION
 		{ "finject",       test_finject       },
 #endif
+		{ "hash",          test_hashtable     },
 		{ "list",          test_list          },
 		{ "locality",      test_locality      },
 		{ "lockers",       test_lockers       },
@@ -78,7 +79,6 @@ const struct m0_test_suite m0_klibm0_ut = {
 		{ "uuid",          m0_test_lib_uuid   },
 		{ "vec",           test_vec           },
 		{ "zerovec",       test_zerovec       },
-		{ "hash",          test_hashtable     },
 		{ NULL,            NULL               }
 	}
 };
diff --git a/lib/ut/main.c b/lib/ut/main.c
index e5defa9..03cd784 100644
--- a/lib/ut/main.c
+++ b/lib/ut/main.c
@@ -66,6 +66,7 @@ const struct m0_test_suite libm0_ut = {
 		{ "finject",          test_finject       },
 #endif
 		{ "getopts",          test_getopts       },
+		{ "hash",	      test_hashtable     },
 		{ "list",             test_list          },
 		{ "locality",         test_locality      },
 		{ "lockers",          test_lockers       },
@@ -84,7 +85,6 @@ const struct m0_test_suite libm0_ut = {
 		{ "uuid",             m0_test_lib_uuid   },
 		{ "vec",              test_vec           },
 		{ "zerovec",          test_zerovec       },
-		{ "hash",	      test_hashtable     },
 		{ NULL,               NULL               }
 	}
 };
