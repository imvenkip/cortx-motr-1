RB: r/865
---
 conf/confc.h | 4 +++-
 1 file changed, 3 insertions(+), 1 deletion(-)

diff --git a/conf/confc.h b/conf/confc.h
index 4ed19c2..0c74a86 100644
--- a/conf/confc.h
+++ b/conf/confc.h
@@ -395,6 +395,8 @@ struct c2_confc {
 	 * @see c2_confc_ctx
 	 */
 	uint32_t            cc_nr_ctx;
+	/** Magic number. */
+	uint64_t            cc_magix;
 };
 
 /**
@@ -471,7 +473,7 @@ struct c2_confc_ctx {
 	 * `object unpinned' events.
 	 */
 	struct c2_clink          fc_clink;
-	/** Magic value. */
+	/** Magic number. */
 	uint64_t                 fc_magix;
 };
 
