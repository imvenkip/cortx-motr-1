 DEF(confx_object, RECORD,
     _(o_key, c2_conf_buf),
     _(o_val, objval));
@@ -190,5 +190,5 @@ DEF(c2_conf_fetch, RECORD,
 DEF(c2_conf_fetch_resp, RECORD,
     /** Result of configuration retrieval (-Exxx = failure, 0 = success). */
     _(fr_rc,   U32),
