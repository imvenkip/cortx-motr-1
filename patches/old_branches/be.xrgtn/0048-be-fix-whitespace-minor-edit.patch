 2 files changed, 16 insertions(+), 14 deletions(-)

diff --git a/be/seg.c b/be/seg.c
index eb9e81a..3663c22 100644
--- a/be/seg.c
+++ b/be/seg.c
@@ -49,7 +49,7 @@ static void seg_io_prepare(struct m0_be_seg *seg, struct m0_stob_io *io)
 
 	io->si_flags  = 0;
 
-	io->si_fol_rec_part = (void *)1; /* XXX */
+	io->si_fol_rec_part = (void *)1; /* XXX log/payload/opcode-spec-dat */
 
 	io->si_user.ov_vec.v_nr    = 1;
 	io->si_user.ov_vec.v_count = count;
@@ -61,7 +61,7 @@ static void seg_io_prepare(struct m0_be_seg *seg, struct m0_stob_io *io)
 }
 
 static void seg_header_prepare(struct m0_be_seg *seg, struct m0_stob_io *io,
-				m0_bcount_t size)
+			       m0_bcount_t size)
 {
 	struct be_seg_hdr *header = (struct be_seg_hdr *)seg->bs_addr;
 
@@ -86,7 +86,7 @@ static int seg_header_write(struct m0_be_seg *seg, m0_bcount_t size)
 	m0_clink_add(&io.si_wait, &clink);
 
 	rc = m0_stob_io_launch(&io, seg->bs_stob, NULL, NULL);
-	M0_ASSERT(rc != 0); /* XXX FIXME */
+	M0_ASSERT(rc == 0); /* XXX FIXME handle errors */
 
 	m0_chan_wait(&clink);
 	m0_clink_del(&clink);
@@ -139,13 +139,13 @@ static int stob_io_single_read(void *buf, m0_bcount_t bufsz,
 			       struct m0_stob *stob,
 			       m0_bcount_t offs, uint32_t pgshift)
 {
-	size_t             pgsz;
-	m0_bcount_t        rdcnt[1];    /* sectors counts */
-	m0_bcount_t        rdoffs[1];   /* offsets */
-	void              *rdbufpk[1];  /* "packed" addresses */
-	struct m0_stob_io  io;
-	struct m0_clink    clink;
-	int                rc;
+	size_t            pgsz;
+	m0_bcount_t       rdcnt[1];    /* sectors counts */
+	m0_bcount_t       rdoffs[1];   /* offsets */
+	void             *rdbufpk[1];  /* "packed" addresses */
+	struct m0_stob_io io;
+	struct m0_clink   clink;
+	int               rc;
 
 	pgsz = 1 << pgshift;
 
@@ -250,11 +250,13 @@ void m0_be_reg_put(const struct m0_be_reg *reg)
 int m0_be__reg_read(struct m0_be_seg *seg, struct m0_be_op *op,
 		    struct m0_be_reg *regs, m0_bcount_t nr)
 {
+	return -1; /*XXX*/
 }
 
 int m0_be__reg_write(struct m0_be_seg *seg, struct m0_be_op *op,
 		     struct m0_be_reg *regs, m0_bcount_t nr)
 {
+	return -1; /*XXX*/
 }
 
