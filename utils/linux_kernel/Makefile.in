KUTC2_SRCDIR = @SRCDIR@/utils/linux_kernel

obj-m       := kutc2.o kc2ctl.o
kutc2-y     := main.o
kc2ctl-y    := c2ctl.o finject_debugfs.o

EXTRA_CFLAGS = -DHAVE_CONFIG_H -iquote @SRCDIR@ @KCFLAGS@

prepare:

distclean clean:
	$(RM) $(shared_ut) Module.markers Module.symvers modules.order
	$(MAKE) -C @LINUX_OBJ@ M=`pwd` clean;

distdir:
	cp $(KUTC2_SRCDIR)/Makefile.in $(KUTC2_SRCDIR)/main.c \
@top_builddir@/@PACKAGE@-@PACKAGE_VERSION@/utils/linux_kernel
