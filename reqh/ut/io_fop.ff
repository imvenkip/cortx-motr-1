/**
   @addtogroup reqh
   @{
 */

/** @file io_fop.ff
 *
 * <b>Simplistic IO fop formats</b>
 *
 */

record {
	u64 f_seq;
	u64 f_oid
} stob_io_fop_fid;

sequence {
	u32 fi_count;
	u8  fi_buf
} m0_fi_value;

record {
	stob_io_fop_fid fiw_object;
	m0_fi_value     fiw_value
} m0_stob_io_write;

record {
	u32 fiwr_rc;
	u32 fiwr_count
} m0_stob_io_write_rep;

record {
	stob_io_fop_fid fir_object
} m0_stob_io_read;

record {
	u32 firr_rc;
	u32 firr_count;
	m0_fi_value firr_value
} m0_stob_io_read_rep;

record {
	stob_io_fop_fid fic_object
} m0_stob_io_create;

record {
	u32 ficr_rc
} m0_stob_io_create_rep;

/** @} end of reqh group */

