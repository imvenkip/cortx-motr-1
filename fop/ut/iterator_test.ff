FOPDEP("fid/fid.ff");

DEF(c2_fop_seg, RECORD,
    _(fs_offset, U64),
    _(fs_count, U64));

DEF(c2_fop_vec, SEQUENCE,
    _(fv_count, U32),
    _(fv_seg, c2_fop_seg));

DEF(c2_fop_optfid, UNION,
    _(fo_present, U32),
    _case(1, fo_fid, c2_fop_fid),
    _case(0, fo_none, VOID));

DEF(c2_fop_fid_typedef, TYPEDEF,
    _(, c2_fop_optfid));

DEF(c2_fop_recursive1, RECORD,
    _(fr_fid, c2_fop_fid),
    _(fr_seq, c2_fop_vec),
    _(fr_unn, c2_fop_optfid));

DEF(c2_fop_recursive2, RECORD,
    _(fr_fid, c2_fop_fid),
    _(fr_seq, c2_fop_recursive1));

DEF(c2_fop_iterator_test, RECORD,
    _(fit_fid, c2_fop_fid),
    _(fit_vec, c2_fop_vec),
    _(fit_opt0, c2_fop_optfid),
    _(fit_opt1, c2_fop_optfid),
    _(fit_topt, c2_fop_fid_typedef),
    _(fit_rec, c2_fop_recursive2));

