record {
	u64 ff_seq;
	u64 ff_oid
} c2_fop_fid;

record {
	u64 fs_offset;
	u64 fs_count
} c2_fop_seg;

sequence {
	u32        fv_count;
	c2_fop_seg fv_seg
} c2_fop_vec;

record {
	c2_fop_fid fo_fid;
	void       fo_none
} c2_fop_optfid;

record {
	c2_fop_fid    fr_fid;
	c2_fop_vec    fr_seq;
	c2_fop_optfid fr_unn
} c2_fop_recursive1;

record {
	c2_fop_fid        fr_fid;
	c2_fop_recursive1 fr_seq
} c2_fop_recursive2;

record {
	c2_fop_fid         fit_fid;
	c2_fop_vec         fit_vec;
	c2_fop_optfid      fit_opt0;
	c2_fop_optfid      fit_opt1;
	c2_fop_optfid      fit_topt;
	c2_fop_recursive2  fit_rec
} c2_fop_iterator_test;

