require "fid/fid_xc";
require "fid/fid";

record {
	u64                fs_offset;
	u64                fs_count
} m0_fop_seg;

sequence {
	u32                fv_count;
	m0_fop_seg         fv_seg
} m0_fop_vec;

record {
	m0_fid             fo_fid;
	void               fo_none
} m0_fop_optfid;

record {
	m0_fid             fr_fid;
	m0_fop_vec         fr_seq;
	m0_fop_optfid      fr_unn
} m0_fop_recursive1;

record {
	m0_fid             fr_fid;
	m0_fop_recursive1  fr_seq
} m0_fop_recursive2;

record {
	m0_fid             fit_fid;
	m0_fop_vec         fit_vec;
	m0_fop_optfid      fit_opt0;
	m0_fop_optfid      fit_opt1;
	m0_fop_optfid      fit_topt;
	m0_fop_recursive2  fit_rec
} m0_fop_iterator_test;

